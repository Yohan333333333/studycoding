package com.javalec.base;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        // 구구단 2단 출력
		
		//
		for(int j=2; j<=9; j++) {
			for(int i=1; i<=9; i++) {
				System.out.println(j + "X"+ i + "=" + (5*i));
			}
		}
	}
}
			
		
		
		
		
		
		
		
	

